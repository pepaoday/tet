@model IEnumerable<DoAnPhanMem.Models.DANHMUCDV>
@{
    ViewBag.Title = "Danh Mục";
    Layout = "~/Views/Shared/_Layoutweb.cshtml";
}
<!-- Start header -->
<div class="all-page-title page-breadcrumb">
    <div class="container text-center">
        <div class="row">
            <div class="col-lg-12   ">
                <h1 style="font-family:'Times New Roman', Times, serif">DANH MỤC DỊCH VỤ</h1>
            </div>
        </div>
    </div>
</div>
<!-- End header -->
<!-- Start Gallery -->
<div class="gallery-box">
    <div class="container-fluid">
        <div class="row">
            <div class="col-lg-12">
                <div class="heading-title text-center">
                    <h2 style="font-family: 'Times New Roman', Times, serif; font-weight:bold">C11 SPA</h2>
                    <p> Ngày xưa sắc đẹp trời cho - Ngày nay sắc đẹp là do người làm</p>
                </div>
            </div>
        </div>
        <div class="tz-gallery">
            <div class="row">
                @foreach (var item in Model)
                {
                    <div class="col-sm-12 col-md-4 col-lg-4">
                        <a class="lightbox" href="~/Images/@item.IMG">
                            <h2 style="text-align: center; text-transform: uppercase; font-family: 'Times New Roman', Times, serif ; color:maroon"> <strong> @item.TENDM </strong> </h2>
                            <img class="img-fluid" src="~/Images/@item.IMG" alt="Images" style="height:350px;width:500px">
                        </a>
                    </div>
                }
            </div>
        </div>
    </div>
</div>
<!-- End Gallery -->
